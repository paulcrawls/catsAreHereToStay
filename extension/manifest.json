{
  "name": "Cats",
  "description": "Add more cats",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "background": { "scripts": ["background.js"] },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["cats.css"],
      "js": ["lib/jquery.min.js", "lib/jquery-ui.min.js"]
    }
  ],
  "browser_action": {
    "default_title": "Add more cats"
  },
  "web_accessible_resources": ["cats.jpg"],
  "manifest_version": 2
}
